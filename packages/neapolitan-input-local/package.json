{
  "name": "neapolitan-input-local",
  "version": "0.1.0-canary.1",
  "description": "Local input plugin for Neapolitan",
  "keywords": [
    "neapolitan-plugin"
  ],
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kijv/neapolitan.git",
    "directory": "packages/neapolitan-input-local"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && BUILD=1 rolldown -m -c rolldown.config.ts",
    "dev": "rolldown -c rolldown.config.ts --watch"
  },
  "devDependencies": {
    "@rolldown/pluginutils": "catalog:bundler",
    "@rollup/pluginutils": "catalog:bundler",
    "neapolitan": "workspace:*",
    "package-directory": "^8.1.0",
    "rolldown": "catalog:bundler",
    "rolldown-plugin-dts": "catalog:bundler",
    "rollup-plugin-swc3": "catalog:bundler"
  }
}
