{
  "name": "neapolitan-output-mdx",
  "description": "MDX output plugin for Neapolitan",
  "keywords": [
    "neapolitan-plugin"
  ],
  "version": "0.1.0-canary.0",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kijv/neapolitan.git",
    "directory": "packages/neapolitan-output-mdx"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./compiler": {
      "types": "./dist/compiler.d.ts",
      "default": "./dist/compiler.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && BUILD=1 rolldown -m -c rolldown.config.ts",
    "dev": "rolldown -c rolldown.config.ts --watch"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@rolldown/pluginutils": "catalog:bundler",
    "@rollup/pluginutils": "catalog:bundler",
    "@types/mdast": "^4.0.4",
    "@types/react": "^19.1.9",
    "estree-util-value-to-estree": "^3.4.0",
    "neapolitan": "workspace:*",
    "rolldown": "catalog:bundler",
    "rolldown-plugin-dts": "catalog:bundler",
    "rollup-plugin-swc3": "catalog:bundler",
    "unified": "^11.0.5"
  }
}
